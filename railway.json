{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python setup_railway.py && gunicorn --bind 0.0.0.0:$PORT --workers 2 --timeout 120 frappe_app:application",
    "healthcheckPath": "/api/health",
    "healthcheckTimeout": 30
  }
} 